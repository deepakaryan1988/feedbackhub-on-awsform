# MongoDB Atlas Connection String
# Replace the placeholders with your actual MongoDB Atlas credentials
MONGO_URL=mongodb+srv://<USERNAME>:<PASSWORD>@<CLUSTER_URL>/feedbackhub?retryWrites=true&w=majority

# For Docker Compose (optional)
MONGO_USERNAME=<USERNAME>
MONGO_PASSWORD=<PASSWORD>
MONGO_CLUSTER_URL=<CLUSTER_URL>

# Application Port (optional, defaults to 3000)
PORT=3000

# Node Environment
NODE_ENV=development

# Next.js Telemetry (optional)
NEXT_TELEMETRY_DISABLED=1 